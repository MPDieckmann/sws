{
  "compilerOptions": {
    "module": "None",
    "moduleResolution": "classic",
    "target": "ESNext",
    "outFile": "../serviceworker.js",
    "sourceMap": true,
    "allowJs": false
  },
  "include": [
    "ts/server/*.ts",
    "ts/index.ts",
    "ts/routes/*.ts"
  ],
  "exclude": [
    "**/node_modules",
    "**/*.js",
    "**/*.d.ts"
  ]
}